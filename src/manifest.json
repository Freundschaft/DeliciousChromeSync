{
  "name": "Chrome Delicious Sync",
  "version": "1.2",
  "description": "syncs booksmarks from Chrome with Delicious Bookmarking Service, based on the Chrome Extension Sample",
  "permissions": [
	"identity",
	"tabs",
	"https://delicious.com/auth/authorize",
	"https://avosapi.delicious.com/api/v1/oauth/token",
    "bookmarks",
	"https://api.del.icio.us/",
    "http://localhost/"
  ],
  "browser_action": {
      "default_title": "Chrome Delicious Sync",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "a9adbe0c558cc27a3ec117e80dd29353",
    "scopes": [
      "https://delicious.com/auth/authorize"
    ]
  },
  "key": ""
}
